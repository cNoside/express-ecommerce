    "argon2": "0.28.7",
    "class-validator": "0.13.2",
    "express": "4.18.1",
    "express-async-handler": "1.2.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "swagger-ui-express": "4.5.0",
