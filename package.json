    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "dotenv": "16.0.1",
    "eslint": "8.22.0",
    "prisma": "4.2.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
